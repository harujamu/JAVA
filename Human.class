package extend.human;

public class Human {
	private String name;
	protected int age;
	
//	引数にnameとageを持つパブリックメソッドHuman定義
	public Human(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
//	nameのgetterメソッドgetName定義
	public String getName() {
		return this.name;
	}
	
//	ageのgetterメソッドgetAge定義
	public int getAge() {
		return this.age;
	}
}